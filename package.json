{
  "name": "web-render",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --content-base dist",
    "deploy": "webpack -p && cf push houndstooth -b https://github.com/cloudfoundry/staticfile-buildpack.git -m 64m",
    "test": "ava **/test/**/*.js",
    "setup-submodules": "git submodule update --init --recursive",
    "push-all": "git push && git submodule foreach git push",
    "pull-all": "git pull -r && git submodule foreach git pull -r"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "file-saver": "^1.3.3",
    "path": "^0.12.7",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
