{
  "name": "web-render",
  "version": "1.0.0",
  "repository": "https://github.com/houndstooth/web-render",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --content-base dist",
    "lint-fix": "eslint . --fix",
    "unit": "babel-node ./test/unit/unit.js",
    "cover": "babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./test/unit/unit.js --config ./test/unit/.istanbul.yml",
    "check-coverage": "babel-node ./node_modules/babel-istanbul/lib/cli.js check-coverage ./test/unit/coverage/coverage.json --config ./test/unit/.istanbul.yml",
    "integration": "karma start ./test/integration/karmaRun.conf.js",
    "test-server": "karma start ./test/integration/karmaServer.conf.js",
    "build": "webpack -p"
  },
  "dependencies": {
    "file-saver": "^1.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "canteen": "^1.0.5",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-webpack": "^2.0.4",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
