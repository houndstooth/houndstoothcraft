{
  "name": "web-render",
  "version": "1.0.0",
  "repository": "https://github.com/houndstooth/web-render",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config build/webpack.prod.js",
    "start": "webpack --config build/webpack.dev.js & node server.js &",
    "lint-fix": "eslint . --fix",
    "unit": "babel-node node_modules/jasmine/bin/jasmine.js",
    "cover": "babel-node node_modules/babel-istanbul/lib/cli.js cover node_modules/jasmine/bin/jasmine.js --config test/unit/.istanbul.yml",
    "check-coverage": "babel-node node_modules/babel-istanbul/lib/cli.js check-coverage test/unit/coverage/coverage.json --config test/unit/.istanbul.yml",
    "integration": "karma start test/integration/karma.run.js",
    "test-server": "karma start test/integration/karma.server.js"
  },
  "dependencies": {
    "file-saver": "^1.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-webpack": "^2.0.4",
    "uglify-js": "^3.1.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
