{
  "name": "web-render",
  "version": "1.0.0",
  "repository": "https://github.com/houndstooth/web-render",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --content-base dist",
    "deploy": "sh ./bin/deploy.sh",
    "integration-test-server": "karma start ./test/integration/karma-server.conf.js",
    "setup": "sh ./bin/setup.sh",
    "push-all": "git submodule foreach git push && git push",
    "pull-all": "git pull -r && git submodule foreach git pull -r",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "unit": "babel-node ./test/src/unit.js",
    "test": "npm run coverage && npm run integration",
    "integration": "karma start ./test/integration/karma-single-headless-cli-ci-report-slow.conf.js",
    "coverage": "babel-node ./node_modules/babel-istanbul/lib/cli.js cover ./test/src/unit.js",
    "ci": "npm run unit && npm run integration",
    "startup": "sh ./bin/startup.sh",
    "shutdown": "lsof -ti:8080 | xargs kill && lsof -ti:9876 | xargs kill"
  },
  "dependencies": {
    "file-saver": "^1.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "canteen": "^1.0.5",
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.7.0",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-webpack": "^2.0.4",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
